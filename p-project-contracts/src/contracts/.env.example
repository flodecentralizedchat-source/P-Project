# Example environment configuration for P-Project deployment
# Copy this file to .env and fill in your actual values

# Private key for the wallet that will deploy the contracts
# WARNING: Keep this secret and never commit to version control
PRIVATE_KEY=your_private_key_here

# RPC endpoints for different networks
BSC_TESTNET_URL=https://data-seed-prebsc-1-s1.binance.org:8545
BSC_MAINNET_URL=https://bsc-dataseed.binance.org/
ETH_TESTNET_URL=https://goerli.infura.io/v3/YOUR_INFURA_PROJECT_ID
ETH_MAINNET_URL=https://mainnet.infura.io/v3/YOUR_INFURA_PROJECT_ID

# Etherscan API key for contract verification
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Initial liquidity amounts (in wei/lowest denomination)
INITIAL_LIQUIDITY_TOKEN_AMOUNT=5000000000000000000000000
INITIAL_LIQUIDITY_USDT_AMOUNT=50000000000

# DEX router addresses (example for PancakeSwap)
PANCAKE_ROUTER_ADDRESS=0x10ED43C718714eb63d5aA57B78B54704E256024E
UNISWAP_ROUTER_ADDRESS=0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D